pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git branch: 'pipeline', url: 'https://github.com/MarcioDT/teste-api-ebac.git'                
            }
        }
        stage('Instalando Dependencias') {
            steps {
               bat 'npm install'
            }
        }        
        stage('Teste') {
            steps {
               bat '''set NO_COLOR=1
                npm run 
                cy:run'''
            }
        }
    }
}